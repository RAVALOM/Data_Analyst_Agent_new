{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "100mb",
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "build": {
    "env": {
      "PIP_INSTALL": "pip install -r requirements.txt && python3 pre_cache_models.py"
    }
  }
}
